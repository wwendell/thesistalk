  file = {Snapshot:/home/wendell/.mozilla/firefox/jgng9ajz.default/zotero/storage/NWXMNRIB/C877.html:;Full Text PDF:/home/wendell/.mozilla/firefox/jgng9ajz.default/zotero/storage/S3IMZZJD/Garner and Burg - 1994 - Macromolecular crowding and confinement in cells e.pdf:application/pdf},
  pmid = {8178962}
