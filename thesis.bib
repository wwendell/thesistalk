@article{mikhailov_hydrodynamic_2015,
  title = {Hydrodynamic collective effects of active protein machines in solution and lipid bilayers},
  volume = {112},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1506825112},
  abstract = {The cytoplasm and biomembranes in biological cells contain large numbers of proteins that cyclically change their shapes. They are molecular machines that can function as molecular motors or carry out various other tasks in the cell. Many enzymes also undergo conformational changes within their turnover cycles. We analyze the advection effects that nonthermal fluctuating hydrodynamic flows induced by active proteins have on other passive molecules in solution or membranes. We show that the diffusion constants of passive particles are enhanced substantially. Furthermore, when gradients of active proteins are present, a chemotaxis-like drift of passive particles takes place. In lipid bilayers, the effects are strongly nonlocal, so that active inclusions in the entire membrane contribute to local diffusion enhancement and the drift. All active proteins in a biological cell or in a membrane contribute to such effects and all passive particles, and the proteins themselves, will be subject to them.},
