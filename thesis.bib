@Article{Mukrasch2009,
  author =       {Mukrasch, Marco D and Bibow, Stefan and Korukottu, Jegannath and Jeganathan, Sadasivam and Biernat, Jacek and Griesinger, Christian and Mandelkow, Eckhard and Zweckstetter, Markus},
  title =        {{Structural polymorphism of 441-residue tau at single residue resolution.}},
  volume =       {7},
  number =       {2},
  pages =        {e34},
  abstract =     {Alzheimer disease is characterized by abnormal protein deposits in the brain, such as extracellular amyloid plaques and intracellular neurofibrillary tangles. The tangles are made of a protein called tau comprising 441 residues in its longest isoform. Tau belongs to the class of natively unfolded proteins, binds to and stabilizes microtubules, and partially folds into an ordered beta-structure during aggregation to Alzheimer paired helical filaments (PHFs). Here we show that it is possible to overcome the size limitations that have traditionally hampered detailed nuclear magnetic resonance (NMR) spectroscopy studies of such large nonglobular proteins. This is achieved using optimal NMR pulse sequences and matching of chemical shifts from smaller segments in a divide and conquer strategy. The methodology reveals that 441-residue tau is highly dynamic in solution with a distinct domain character and an intricate network of transient long-range contacts important for pathogenic aggregation. Moreover, the single-residue view provided by the NMR analysis reveals unique insights into the interaction of tau with microtubules. Our results establish that NMR spectroscopy can provide detailed insight into the structural polymorphism of very large nonglobular proteins.},
  bdsk-url-1 =   {http://dx.doi.org/10.1371/journal.pbio.1000034},
  date =         {2009-02},
  doi =          {10.1371/journal.pbio.1000034},
  file =         {:home/poyiho/Desktop/papers/Mukrasch et al.\_2009\_Structural polymorphism of 441-residue tau at single residue resolution.pdf:pdf},
  issn =         {1545-7885},
  journaltitle = {PLoS Biology},
  keywords =     {Alzheimer Disease,Alzheimer Disease: genetics,Alzheimer Disease: metabolism,Amino Acid Sequence,Animals,Axons,Axons: metabolism,Humans,Microtubules,Microtubules: chemistry,Microtubules: metabolism,Molecular Sequence Data,Neurofibrillary Tangles,Neurofibrillary Tangles: genetics,Neurofibrillary Tangles: metabolism,Nuclear Magnetic Resonance, Biomolecular,Nuclear Magnetic Resonance, Biomolecular: methods,Protein Conformation,Protein Folding,Protein Isoforms,Protein Structure, Secondary,Swine,tau Proteins,tau Proteins: chemistry,tau Proteins: genetics,tau Proteins: metabolism},
  pmid =         {19226187}
}

