  keywords =     {Computational Biology,Computational Biology: methods,Magnetic Resonance Spectroscopy,Protein Conformation,Protein Denaturation,Protein Folding,Proteins,Proteins: chemistry,Proteins: metabolism,Reproducibility of Results,Scattering, Radiation,X-Rays},
