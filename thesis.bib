@Article{dunbrack_jr._bayesian_1997,
  author =       {Dunbrack Jr., Roland L and Cohen, Fred E},
  title =        {{Bayes}ian statistical analysis of protein side-chain rotamer preferences},
  volume =       {6},
  number =       {8},
  pages =        {1661--1681},
  abstract =     {We present a Bayesian statistical analysis of the conformations of side chains in proteins from the Protein Data Bank. This is an extension of the backbone-dependent rotamer library, and includes rotamer populations and average x angles for a full range of {\o},$\psi$ values. The Bayesian analysis used here provides a rigorous statistical method for taking account of varying amounts of data. Bayesian statistics requires the assumption of a prior distribution for parameters over their range of possible values. This prior distribution can be derived from previous data or from pooling some of the present data. The prior distribution is combined with the data to form the posterior distribution, which is a compromise between the prior distribution and the data. For the X2, X3, and X4 rotamer prior distributions, we assume that the probability of each rotamer type is dependent only on the previous X rotamer in the chain. For the backbone-dependence of the x1 rotamers, we derive prior distributions from the product of the {\o}-dependent and $\psi$-dependent probabilities. Molecular mechanics calculations with the {CHARMM22} potential show a strong similarity with the experimental distributions, indicating that proteins attain their lowest energy rotamers with respect to local backbone-side-chain interactions. The new library is suitable for use in homology modeling, protein folding simulations, and the refinement of X-ray and {NMR} structures.},
  bdsk-url-1 =   {http://dx.doi.org/10.1002/pro.5560060807},
  copyright =    {Copyright © 2008 The Protein Society},
  date =         {1997-08},
  doi =          {10.1002/pro.5560060807},
  file =         {:home/poyiho/Desktop/papers/Dunbrack, Cohen\_1997\_Bayesian statistical analysis of protein side-chain rotamer preferences.pdf:pdf},
  issn =         {{1469-896X}},
  journaltitle = {Protein Science},
  keywords =     {Bayesian statistics, molecular mechanics, protein structure, rotamers, side chains},
  pmid =         {9260279}
}

